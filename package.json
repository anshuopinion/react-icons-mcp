{
	"name": "react-icons-mcp",
	"version": "0.1.2",
	"description": "Model Context Protocol server for React Icons integration",
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"react-icons-mcp": "dist/cli.js"
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsup --dts",
		"type-check": "tsc --noEmit",
		"start": "node dist/cli.js",
		"start:cli": "cross-env NODE_ENV=cli node dist/cli.js",
		"start:http": "node dist/cli.js",
		"health-check": "node dist/health-check.js",
		"dev": "cross-env NODE_ENV=development tsup --watch",
		"dev:cli": "cross-env NODE_ENV=development tsup --watch -- --stdio",
		"lint": "eslint . --ext .ts",
		"format": "prettier --write \"src/**/*.ts\"",
		"prepack": "npm run build",
		"pub:release": "npm run build && npm publish",
		"test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yourusername/react-icons-mcp.git"
	},
	"keywords": [
		"react-icons",
		"mcp",
		"typescript",
		"ai"
	],
	"author": "",
	"license": "MIT",
	"dependencies": {
		"@modelcontextprotocol/sdk": "^1.8.0",
		"@types/express": "^4.17.21",
		"@types/yargs": "^17.0.33",
		"cross-env": "^7.0.3",
		"dotenv": "^16.4.7",
		"express": "^4.21.2",
		"js-yaml": "^4.1.0",
		"node-fetch": "^3.3.2",
		"react-icons": "^5.5.0",
		"yargs": "^17.7.2",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/jest": "^29.5.14",
		"@types/js-yaml": "^4.0.9",
		"@types/node": "^20.17.30",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"eslint": "^8.52.0",
		"jest": "^29.7.0",
		"jest-environment-node": "^29.7.0",
		"prettier": "^3.0.3",
		"ts-jest": "^29.3.2",
		"tsup": "^7.2.0",
		"typescript": "^5.2.2"
	}
}
